@model AlmacenajeUSACR.Models.Articulo_retirado

@{
    ViewData["Title"] = "Retiro";
}

<h1>Retiro</h1>

<h4>Articulo_retirado</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Retiro">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Codigo_cliente" class="control-label"></label>
                <input asp-for="Codigo_cliente" class="form-control" />
                <span asp-validation-for="Codigo_cliente" class="text-danger"></span>
            </div></br>
            <div class="form-group">
                <input type="submit" value="Buscar" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
