@model AlmacenajeUSACR.Models.Transportista

@{
    ViewData["Title"] = "REgistro transportista";
}

<h1>Registar de transportista</h1>

<h4>Transportista</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Nombre_empresa" class="control-label"></label>
                <input asp-for="Nombre_empresa" class="form-control" />
                <span asp-validation-for="Nombre_empresa" class="text-danger"></span>
            </div></br>
            <div class="form-group">
                <input type="submit" value="Guardar" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div></br>

<div>
    <a asp-controller="Home" asp-action="Index">Regresar</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
